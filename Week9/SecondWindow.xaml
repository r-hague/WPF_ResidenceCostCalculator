<Window x:Class="Week9.SecondWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week9"
        mc:Ignorable="d"
        Title="SecondWindow" Height="300" Width="600">
    <Grid>
        <StackPanel>
            <Label Name="DormLabel" Content="(Dorm)" Margin="20" FontSize="30" HorizontalAlignment="Center"/>
            <Label Name="MealLabel" Content="(Meal)" Margin="20" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            <Label Name="CostLabel" Content="(Total)" Margin="20" FontSize="30" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
